{
  "buildCommand": "cd bff-client && npm run build",
  "outputDirectory": "bff-client/dist",
  "devCommand": "cd bff-client && npm run dev",
  "installCommand": "cd bff-client && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
